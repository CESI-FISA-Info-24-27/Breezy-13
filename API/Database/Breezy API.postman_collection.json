		{
			"name": "Authentication",
			"item": [
				{
					"name": "Login",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Vérifie si la réponse contient un token\r",
									"if (pm.response.code === 200) {\r",
									"    const jsonData = pm.response.json();\r",
									"    if (jsonData.token) {\r",
									"        // Enregistre le token dans une variable d'environnement\r",
									"        pm.environment.set(\"token\", jsonData.token);\r",
									"        console.log(\"Token enregistré :\", jsonData.token);\r",
									"    } else {\r",
									"        console.log(\"Aucun token trouvé dans la réponse.\");\r",
									"    }\r",
									"} else {\r",
									"    console.log(\"Erreur lors de la connexion :\", pm.response.code);\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"username\": \"admin\",\n  \"password\": \"choucroute\"\n}"
						},
						"url": {
							"raw": "{{localhost}}/auth",
							"host": [
								"{{localhost}}"
							],
							"path": [
								"auth"
							]
						}
					},
					"response": []
				},
				{
					"name": "Refresh Token",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{token}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/auth/refresh",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"auth",
								"refresh"
							]
						}
					},
					"response": []
				},
